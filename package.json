{
  "name": "allympian",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn schema && next build",
    "postbuild": "next-sitemap --config custom-next-sitemap.js",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --config jest.config.ts",
    "prettier": "prettier --write .",
    "editor": "pnpify --sdk vscode",
    "generate": "gqless generate",
    "schema": "NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' NODE_PATH=./graphql ts-node-script --transpile-only graphql/schema",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.6",
    "@gqless/logger": "^2.0.11",
    "@gqless/react": "^2.0.15",
    "@gqless/subscriptions": "^1.0.11",
    "@prisma/client": "2.21.2",
    "@stitches/react": "^0.1.9",
    "apollo-server-micro": "^2.23.0",
    "gqless": "^2.0.13",
    "graphql-scalars": "^1.9.0",
    "next": "10.1.3",
    "next-i18next": "^8.2.0",
    "next-pwa": "^5.2.14",
    "next-seo": "^4.24.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@gqless/cli": "^2.0.15",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@yarnpkg/pnpify": "^3.0.0-rc.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "next-sitemap": "^1.6.25",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
